    "type": "symfony-bundle",
    "require": {
        "google/apiclient": "^2.2",
        "symfony/framework-bundle": "^2.8 || ^3.0 || ^4.0 || ^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "symfony/yaml": "^2.8 || ^3.0 || ^4.0 || ^5.0"
